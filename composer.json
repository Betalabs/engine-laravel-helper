{
  "name": "betalabs/engine-laravel-helper",
  "description": "Engine Laravel Helper provides essential functionalities to work with Engine apps on Laravel.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Betalabs Tecnologia LTDA.",
      "email": "betalabs@betalabs.com.br"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/passport": "^10.0",
    "myclabs/php-enum": "^1.5",
    "betalabs/engine-phpsdk": "^2.0",
    "laravel/legacy-factories": "^1.0",
    "doctrine/dbal": "^2.11|^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.2",
    "mockery/mockery": "^1.0",
    "phpunit/phpunit": "^8.4"
  },
  "autoload": {
    "psr-4": {
      "Betalabs\\LaravelHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Betalabs\\LaravelHelper\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Betalabs\\LaravelHelper\\LaravelHelperServiceProvider",
        "Betalabs\\LaravelHelper\\LaravelHelperEventProvider",
        "Betalabs\\LaravelHelper\\PerennialAccessTokenProvider"
      ]
    }
  }
}
